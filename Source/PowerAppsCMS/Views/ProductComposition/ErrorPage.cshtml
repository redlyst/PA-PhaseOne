@{
    ViewBag.Title = "Error";
}

<div class="body-content">
    <h1 class="title text-danger">Error</h1>
    <p class="text-danger">@ViewBag.Exception</p>
    <p class="text-danger">@ViewBag.ExistProductComposition</p>
</div>
<div class="btn-back">
    <a href="@Url.Action("Details", "Product", new { id = @ViewBag.ProductID})" class="btn btn-primary">Back to Product Detail</a>
</div>



